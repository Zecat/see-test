{
  "name": "cmut",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "includes"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "subgroup"
          }
        }
      ]
    },
    "includes": {
      "type": "PATTERN",
      "value": "[^-]*"
    },
    "subgroup": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_subgroup_line"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "function_alias_definition"
              },
              {
                "type": "SYMBOL",
                "name": "_c_code"
              },
              {
                "type": "SYMBOL",
                "name": "call_expression"
              }
            ]
          }
        }
      ]
    },
    "_subgroup_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "- *"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "subgroup_name"
          }
        }
      ]
    },
    "subgroup_name": {
      "type": "PATTERN",
      "value": ".+"
    },
    "function_alias_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "alias_name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "FIELD",
          "name": "function_name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        }
      ]
    },
    "call_expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "FIELD",
          "name": "arguments",
          "content": {
            "type": "SYMBOL",
            "name": "argument_list"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_return_expectation"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_return_expectation": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_return_value_expectation"
          },
          {
            "type": "SYMBOL",
            "name": "_capture_output"
          }
        ]
      }
    },
    "_capture_output": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ">>"
        },
        {
          "type": "FIELD",
          "name": "output",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "string"
              },
              {
                "type": "SYMBOL",
                "name": "varname"
              }
            ]
          }
        }
      ]
    },
    "_return_value_expectation": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "=>"
        },
        {
          "type": "FIELD",
          "name": "return",
          "content": {
            "type": "SYMBOL",
            "name": "_value"
          }
        }
      ]
    },
    "argument_list": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "_expectable_value"
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "SYMBOL",
                        "name": "_expectable_value"
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "_expectable_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "expectation"
        },
        {
          "type": "SYMBOL",
          "name": "_value"
        }
      ]
    },
    "expectation": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "input",
          "content": {
            "type": "SYMBOL",
            "name": "_value"
          }
        },
        {
          "type": "STRING",
          "value": "=>"
        },
        {
          "type": "FIELD",
          "name": "expect",
          "content": {
            "type": "SYMBOL",
            "name": "_value"
          }
        }
      ]
    },
    "_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "char"
        },
        {
          "type": "SYMBOL",
          "name": "array"
        },
        {
          "type": "SYMBOL",
          "name": "null"
        },
        {
          "type": "SYMBOL",
          "name": "address"
        },
        {
          "type": "SYMBOL",
          "name": "varname"
        }
      ]
    },
    "varname": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[A-Za-z0-9_]+"
    },
    "number": {
      "type": "PATTERN",
      "value": "[-]?\\d+"
    },
    "null": {
      "type": "STRING",
      "value": "NULL"
    },
    "address": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "&"
        },
        {
          "type": "SYMBOL",
          "name": "_value"
        }
      ]
    },
    "string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "PATTERN",
          "value": "[^\"]*"
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "char": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "'"
        },
        {
          "type": "PATTERN",
          "value": "[^']?"
        },
        {
          "type": "STRING",
          "value": "'"
        }
      ]
    },
    "array": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "_expectable_value"
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "SYMBOL",
                        "name": "_expectable_value"
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "//"
              },
              {
                "type": "PATTERN",
                "value": ".*"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "/*"
              },
              {
                "type": "PATTERN",
                "value": "[^*]*\\*+([^\\/*][^*]*\\*+)*"
              },
              {
                "type": "STRING",
                "value": "/"
              }
            ]
          }
        ]
      }
    },
    "c_code": {
      "type": "PATTERN",
      "value": ".*"
    },
    "multiline_c_code": {
      "type": "PATTERN",
      "value": "[^%]*[^>]"
    },
    "_c_code": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "PATTERN",
              "value": "%[\\s]*"
            },
            {
              "type": "SYMBOL",
              "name": "c_code"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "PATTERN",
              "value": "<%\\n?"
            },
            {
              "type": "ALIAS",
              "content": {
                "type": "SYMBOL",
                "name": "multiline_c_code"
              },
              "named": true,
              "value": "c_code"
            },
            {
              "type": "STRING",
              "value": "%>"
            }
          ]
        }
      ]
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "PATTERN",
      "value": "[\\s]"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

